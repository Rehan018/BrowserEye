# BrowserEye Environment Configuration
# Copy this file to .env and fill in your API keys

# OpenAI Configuration
VITE_OPENAI_API_KEY=sk-your-openai-api-key-here
VITE_OPENAI_MODEL=gpt-4
VITE_OPENAI_BASE_URL=https://api.openai.com/v1

# Anthropic Claude Configuration
VITE_ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here
VITE_ANTHROPIC_MODEL=claude-3-sonnet-20240229
VITE_ANTHROPIC_BASE_URL=https://api.anthropic.com

# Google Gemini Configuration
VITE_GEMINI_API_KEY=your-gemini-api-key-here
VITE_GEMINI_MODEL=gemini-pro
VITE_GEMINI_BASE_URL=https://generativelanguage.googleapis.com

# Ollama Configuration (Local)
VITE_OLLAMA_BASE_URL=http://localhost:11434
VITE_OLLAMA_MODEL=llama2

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_LOGGING=false
VITE_MOCK_API=false

# Extension Configuration
VITE_EXTENSION_ID=your-extension-id-here
VITE_VERSION=0.1.0

# Analytics (Optional)
VITE_ANALYTICS_ENABLED=false
VITE_ANALYTICS_ID=your-analytics-id-here

# Feature Flags
VITE_ENABLE_AUTONOMOUS_MODE=true
VITE_ENABLE_MEMORY_SYSTEM=true
VITE_ENABLE_SCREENSHOTS=true
VITE_ENABLE_VOICE_COMMANDS=false

# Security Configuration
VITE_CSP_ENABLED=true
VITE_SECURE_STORAGE=true

# Performance Configuration
VITE_MAX_MEMORY_USAGE=100
VITE_REQUEST_TIMEOUT=30000
VITE_MAX_RETRIES=3